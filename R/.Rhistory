library(tcltk)
select_file_or_dir <- function() {
# Abfrage, ob Datei oder Ordner ausgewählt werden soll
choice <- tclvalue(tk_choose.list(choices = c("Datei", "Ordner"), title = "Auswahl treffen", default = 1))
if (choice == "Datei") {
# Datei auswählen
input_path <- choose.file()
} else {
# Ordner auswählen
input_path <- choose.dir()
}
# Rückgabe des ausgewählten Pfads
return(input_path)
}
